{
  "name": "hosted-hubot",
  "version": "2.8.3",
  "private": true,
  "author": "GitHub Inc.",

  "keywords": [
    "github",
    "hubot",
    "campfire",
    "bot"
  ],

  "description": "A simple helpful robot for your Company",

  "licenses": [{
    "type": "MIT",
    "url": "https://github.com/github/hubot/raw/master/LICENSE"
  }],

  "repository" : {
    "type": "git",
    "url": "https://github.com/github/hubot.git"
  },

  "dependencies": {
    <% # Support for ruby 1.8.7 - hashes are not ordered -%>
    <%= @dependencies.sort.map{|k,v| "#{k.inspect}: #{v.inspect}"}.join(",\n    ") %>
  },

  "engines": {
    "node": ">= 0.8.x",
    "npm":  ">= 2.0.x"
  },

  "main": "./index",

  "bin": {
    "hubot": "./bin/hubot"
  }
}
